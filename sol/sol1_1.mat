clear all; close all; clc
load data/hw1_prob1
load('./data/get_DVH_PTV.m')

%% Construct A and b
A=[ ones(N,1) log(k) log(m) log(n) ];
b= log(T);

%% Solve Ax=b by Least-Squares
xhat= A\b
xhat= pinv(A)*b
xhat= inv(A'*A)*A'*b

%% Results
alpha= exp(xhat(1))
beta= xhat(2)
gamma= xhat(3)
delta= xhat(4)

J= norm(A*xhat-b)^2